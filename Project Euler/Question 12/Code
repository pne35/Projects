import math

def triangle(n): 
    return n * (n + 1) // 2

def count_divisors(n):
    count = 0
    root = int(math.sqrt(n))
    for i in range(1, root + 1):
        if n % i == 0:
            count += 2     
    if root * root == n:
        count -= 1         
    return count

n = 1
while True:
    t = triangle(n)
    if count_divisors(t) > 500:
        print("Triangle:", t, "Divisors:", count_divisors(t))
        break
    n += 1

