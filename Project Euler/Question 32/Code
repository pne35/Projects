def isPand(x, y):
    s = str(x) + str(y) + str(x * y)
    return len(s) == 9 and set(s) == set("123456789")

products = set()

for x in range(1, 10):
    for y in range(1000, 10000):
        if isPand(x, y):
            products.add(x * y)
            print(x, y, x*y)

for x in range(10, 100):
    for y in range(100, 1000):
        if isPand(x, y):
            products.add(x * y)
            print(x, y, x*y)

print(sum(products))
